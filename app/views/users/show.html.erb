<table class="profile" summary="Profile information">  <tr>	<td class="main">	  <h1>		<%= gravatar_for @user %>		<%= @user.name %>	  </h1>	  <% unless @user.microposts.empty? %>		<table class="microposts" summary="User microposts">		  	<ul class='micropost'><%= render :partial => @microposts, :collection => @microposts %></ul>		</table>		<%= will_paginate @microposts %>	  <% end %>	</td>	<td class="sidebar round">	  <%= render 'shared/user_info' %> 	</td>  </tr></table>